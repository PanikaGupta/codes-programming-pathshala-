//code to print subset of numbers take one element atmost 2 times
#include <iostream>
using namespace std;
void pass(int i,int temp[],int s,int a[],int n){
    if(i==n){
        cout<<"[";
        for(int j=0;j<s;j++){
            cout<<temp[j];
        }
        
        cout<<"]"<<endl;
        return;
    }
    else{
    pass(i+1,temp,s,a,n);
    temp[s]=a[i];
    pass(i+1,temp,s+1,a,n);
    }
    }
int main() {
    int n;
    cin>>n;
    int* arr = new int[n];
    for(int i=0;i<n;i++){
        cin >>arr[i];
    }
    int* temp = new int[n];
    int s=0;
    
    pass(0,temp,s,arr,n);
    

    return 0;
}
